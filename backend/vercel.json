{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/okrs/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/feedback/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/reviews/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/time/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/ai/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
