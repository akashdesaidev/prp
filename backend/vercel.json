{
  "version": 2,
  "name": "prp-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/okrs/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/feedback/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/reviews/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/time/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/ai/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "github": {
    "enabled": false
  }
}
